{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
  "jsx": "react-native",
  "allowJs": true,
  "moduleResolution": "node16",
  "module": "Node16",
  "esModuleInterop": true,
  "types": ["nativewind/types"],
  "paths": {
    "@/*": [
      "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
